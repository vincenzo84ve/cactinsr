<h1>Listing lineas_despachos</h1>

<table>
  <tr>
    <th>Existencia</th>
    <th>Depacho</th>
    <th>Cantidad</th>
    <th>Esta activo</th>
  </tr>

<% @lineas_despachos.each do |lineas_despacho| %>
  <tr>
    <td><%=h lineas_despacho.existencia_id %></td>
    <td><%=h lineas_despacho.depacho_id %></td>
    <td><%=h lineas_despacho.cantidad %></td>
    <td><%=h lineas_despacho.esta_activo %></td>
    <td><%= link_to 'Show', lineas_despacho %></td>
    <td><%= link_to 'Edit', edit_lineas_despacho_path(lineas_despacho) %></td>
    <td><%= link_to 'Destroy', lineas_despacho, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New lineas_despacho', new_lineas_despacho_path %>