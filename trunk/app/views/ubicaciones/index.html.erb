<h1>Listing ubicaciones</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @ubicaciones.each do |ubicacione| %>
  <tr>
    <td><%=h ubicacione.descripcion %></td>
    <td><%= link_to 'Show', ubicacione %></td>
    <td><%= link_to 'Edit', edit_ubicacione_path(ubicacione) %></td>
    <td><%= link_to 'Destroy', ubicacione, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ubicacione', new_ubicacione_path %>