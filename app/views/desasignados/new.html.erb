<h1>New desasignado</h1>

<% form_for(@desasignado) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :fecha %><br />
    <%= f.date_select :fecha %>
  </p>
  <p>
    <%= f.label :asignado_id %><br />
    <%= f.text_field :asignado_id %>
  </p>
  <p>
    <%= f.label :motivo_id %><br />
    <%= collection_select(:desasignado, :motivo_id, @motivos, :id, :descripcion) %>
  </p>
  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_area :descripcion %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', desasignados_path %>