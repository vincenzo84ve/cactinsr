<h1>Editing factura</h1>

<% form_for(@factura) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :fecha %><br />
    <%= calendar_date_select_tag "factura[fecha]", @factura.fecha.to_s, :time => true %>
  </p>
  <p>
    <%= f.label :proveedor_id %><br />
    <%= f.text_field :proveedor_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @factura %> |
<%= link_to 'Back', facturas_path %>