<h1>New carea</h1>

<% form_for(@carea) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nombre %><br />
    <%= f.text_field :nombre %>
  </p>
  <p>
    <%= f.label :cgeneral_id %><br />
	<%= collection_select(:carea, :cgeneral_id, @cgenerales, :id, :nombre) %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', careas_path %>